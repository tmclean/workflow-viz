{
	"id": "Test Schema 1",
	"stylesheets":[
		"/data/schemas/testSchema1/css/testSchema1.css"
	],

	"nodeTypes":{
		"Node Type 1":{
			"cssClass":     "node",
			"cornerRadius": 10,
			"icon":{
				"cssClass": "icon-img",
				"url":    "/data/schemas/testSchema1/svg/icon.svg",
				"offset":{
					"x": 5,
					"y": 5
				}
			}
		},
		"Node Type 2":{
			"cssClass":     "node2",
			"cornerRadius": 0,
			"icon":{
				"cssClass": "icon-img",
				"url":    "/data/schemas/testSchema1/svg/home.svg",
				"offset":{
					"x": 5,
					"y": 5
				}
			}
		}
	},

	"transitionTypes":{
		"Transition Type 1":{

			"cssClass": "transition",
			
			"toMarker": {
				"url":      "/data/schemas/testSchema1/svg/arrow.svg",
				"selector": "#arrow_x5F_left",
				"cssClass": "to-marker",
				"width":    32,
				"height":   32,
				"anchorX":  10,
				"anchorY":  16,
				"scale":    0.2,
				"rotate":   180
			},

			"fromMarker": {
				"url":      "/data/schemas/testSchema1/svg/arrow.svg",
				"selector": "#arrow_x5F_left",
				"cssClass": "to-marker",
				"width":    32,
				"height":   32,
				"anchorX":  10,
				"anchorY":  16,
				"scale":    0.2,
				"rotate":   0
			}
		}
	}
}